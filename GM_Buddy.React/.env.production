# Production environment variables are set via Docker/deployment
# API calls are proxied through nginx to /api
VITE_API_URL=

# Cognito configuration for Hosted UI (set via Docker environment variables)
VITE_COGNITO_DOMAIN=
VITE_COGNITO_CLIENT_ID=
VITE_COGNITO_REDIRECT_URI=
VITE_COGNITO_LOGOUT_URI=

# Set to 'true' to enable Cognito, 'false' for demo mode
VITE_USE_COGNITO=false
