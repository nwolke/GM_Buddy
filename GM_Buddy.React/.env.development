# API URL for the backend server
VITE_API_URL=http://localhost:5000

# Cognito configuration for Hosted UI
# For local development with Cognito, create a .env.local file with actual values
# .env.local is gitignored and will override these values
VITE_COGNITO_DOMAIN=
VITE_COGNITO_CLIENT_ID=
VITE_COGNITO_REDIRECT_URI=http://localhost:3000/callback
VITE_COGNITO_LOGOUT_URI=http://localhost:3000

# Set to 'true' to enable Cognito, 'false' for demo mode
VITE_USE_COGNITO=false
